{
  "name": "pewpewlogs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --setupFiles dotenv/config --detectOpenHandles",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "build-only": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "cypress": "^13.11.0",
    "eslint": "^9.4.0",
    "eslint-plugin-cypress": "^3.3.0",
    "jest": "~29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "start-server-and-test": "^2.0.4",
    "supertest": "7.0.0",
    "vite": "^5.2.13",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./config/testSetup.js"
    ],
    "resetModules": true,
    "verbose": true
